<div class="main-container">
    {{!-- <h1 class="title">Airspace</h1> --}}
    <div class="host-dash">
        <h1>Host Dashboard</h1>
        <p>Your unique Host ID:</p>
        <h1 class="key">{{uniqueid}}</h1>
        <div class="inner-box">
            <div class="split-left">
                <h3>Surveys</h3>
                <div class="activation-box">
                    {{#each result}}
                        
                        <button type="button" data-target="activate-survey" data-survey-id='{{survey_id}}' class="activate-survey">Activate: {{survey_name}}</button>
                        <button type="button" data-target="results" data-survey-id="{{survey_id}}" class="survey-results">Results for: {{survey_name}}</button>
                        
                        <br>
                        <br>
                    {{/each}}
                </div>
                {{!-- <input type="button" value="Create Survey" onclick="location.replace('http://ec2-18-220-45-149.us-east-2.compute.amazonaws.com/{{uniqueid}}/{{id}}/newsurvey')" class="create-another-survey"> --}}
                 <input type="button" value="Create Survey" onclick="location.replace('http://www.airspace.world/{{uniqueid}}/{{id}}/newsurvey')" class="create-another-survey">
            </div>
            <div class="split-middle">
            </div>
            <div class="split-right">
                <div class="active-wrapper">
                    <h3>Active Users</h3>
                    <div class="active-users">
                        <div class="users">
                            
                        </div>
                    </div>
                </div>
                
            </div>
            
            <div class="table-results">
                <h3> Client Results Table </h3>
                <div data-target='results-table'>
                        
                    </div>
            <div>
        </div>
    </div>
   
    <div id="about-box"></div>
    <div id="how-box"></div>
    <div id="contact-box"></div>
</div>

<script src="../../../javascripts/host-dashboard.js" type='text/javascript'></script>