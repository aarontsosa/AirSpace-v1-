<div class="main-container">
    {{!-- <h1 class="title">Airspace</h1> --}}
    <div class="host-dash">
        <h1>Host Dashboard</h1>
        <p>Your unique Host ID:</p>
        <h1 class="key">{{uniqueid}}</h1>
        <div class="inner-box">
            <div class="split-left">
                <h3>Surveys</h3>
                <div class="activation-box">
                    {{#each result}}
                        <button type="button" data-target="activate-survey" data-survey-id='{{survey_id}}' class="activate-survey">{{survey_name}}</button>
                        <button type="button" data-target="results" data-survey-id="{{survey_id}}">Results for {{survey_name}}</button>
                        <br>
                        <br>
                    {{/each}}
                </div>
                <input type="button" value="Create Survey" onclick="location.replace('http://localhost:3001/host/{{uniqueid}}/{{id}}/newsurvey')" class="create-another-survey">
            </div>
            <div class="split-middle"></div>
            <div class="split-right">
                <div class="active-wrapper">
                    <h3>Active Users</h3>
                    <div class="active-users">
                        <div class="users">
                            
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div data-target='results-table'>
        
    </div>
    <div id="about-box"></div>
    <div id="how-box"></div>
    <div id="contact-box"></div>
</div>

<script src="../../../javascripts/host-dashboard.js" type='text/javascript'></script>